pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}

    parameters {
    string(name: 'NAME', defaultValue: '', description: '')
    string(name: 'AGE', defaultValue: '0', description: '')

    stages {
        stage('build') {
            steps {
	    	
                echo "$NAME"
                echo "$AGE"
            }
        }
    }
}
